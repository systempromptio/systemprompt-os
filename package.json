{
  "name": "@systemprompt/os",
  "version": "0.1.0",
  "description": "An operating system for autonomous agents that run locally, remember persistently, and act purposefully.",
  "type": "module",
  "exports": {
    ".": {
      "types": "./build/index.d.ts",
      "default": "./build/index.js"
    }
  },
  "bin": {
    "systemprompt": "bin/systemprompt",
    "systemprompt-os": "build/index.js"
  },
  "files": [
    "build",
    "README.md",
    "LICENSE",
    ".env.example"
  ],
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "scripts": {
    "setup": "npx tsx scripts/setup.ts",
    "start": "npx tsx scripts/start-all.ts",
    "stop": "npx tsx scripts/stop-all.ts",
    "status": "./systemprompt status",
    "tunnel": "npx tsx scripts/start-tunnel.ts",
    "tunnel:status": "npx tsx scripts/tunnel-status.ts",
    "test": "vitest run",
    "test:unit": "vitest run -c tests/config/vitest.unit.config.ts",
    "test:e2e": "vitest run -c tests/config/vitest.e2e.config.ts",
    "test:coverage": "vitest run --coverage",
    "test:coverage:report": "node scripts/test-with-report.js",
    "test:watch": "vitest watch",
    "test:ui": "vitest --ui",
    "test:tunnel": "node build/scripts/test-tunnel-integrated.js",
    "test:interactive": "cd e2e-test/typescript && npm run test:interactive",
    "test:docs": "cd e2e-test/typescript && npx tsx test-docs-task.ts",
    "test:oauth": "npx tsx tests/manual/test-mcp-oauth-flow.ts",
    "send-push": "npx tsx scripts/send-push-notification.ts",
    "logs": "tail -f logs/*.log",
    "build": "npm run build:main && npm run build:scripts",
    "build:main": "tsc && tsc-alias && chmod +x build/index.js",
    "build:scripts": "[ -d scripts ] && cd scripts && tsc || echo 'No scripts directory to build'",
    "dev": "npm run build && npm run start",
    "clean": "rm -rf build e2e-test/dist node_modules e2e-test/node_modules logs/*",
    "inspector": "npx @modelcontextprotocol/inspector"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/systempromptio/systemprompt-os.git"
  },
  "dependencies": {
    "@google/genai": "latest",
    "cors": "^2.8.5",
    "dotenv": "^16.5.0",
    "express": "^5.1.0",
    "uuid": "^9.0.1",
    "yaml": "^2.8.0",
    "zod": "^3.25.67"
  },
  "devDependencies": {
    "@modelcontextprotocol/sdk": "^1.16.0",
    "@types/cookie-parser": "^1.4.9",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.3",
    "@types/inquirer": "^9.0.8",
    "@types/json-schema": "^7.0.15",
    "@types/node": "^22.15.32",
    "@types/oauth2-server": "^3.0.18",
    "@types/supertest": "^6.0.2",
    "@types/uuid": "^9.0.7",
    "@types/ws": "^8.18.1",
    "@typescript-eslint/eslint-plugin": "^8.34.1",
    "@typescript-eslint/parser": "^8.34.1",
    "@vitest/coverage-v8": "^2.0.0",
    "@vitest/ui": "^2.0.0",
    "chalk": "^5.4.1",
    "esbuild": "^0.25.5",
    "eslint": "^9.29.0",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-prettier": "^5.5.0",
    "inquirer": "^12.7.0",
    "open": "^10.2.0",
    "prettier": "^3.5.3",
    "supertest": "^7.0.0",
    "tsc-alias": "^1.8.16",
    "tsx": "^4.20.3",
    "typescript": "^5.8.3",
    "vitest": "^2.0.0",
    "ws": "^8.18.3"
  },
  "keywords": [
    "systemprompt",
    "mcp",
    "model-context-protocol",
    "mcp-server",
    "mcp-example",
    "typescript",
    "type-safe",
    "coding-agent",
    "claude-code",
    "gemini-cli",
    "orchestrator",
    "example",
    "template",
    "boilerplate"
  ],
  "author": "SystemPrompt",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/systempromptio/systemprompt-mcp-server/issues"
  },
  "homepage": "https://systemprompt.io",
  "engines": {
    "node": ">=18.0.0"
  }
}
